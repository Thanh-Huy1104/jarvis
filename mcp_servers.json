{
  "servers": {
    "desktop": {
      "description": "Local desktop tools (filesystem, docker, web search, etc.)",
      "command": "uv",
      "args_template": ["run", "{script}"],
      "script": "servers/desktop/server.py",
      "enabled": true
    },
    "google_calendar": {
      "description": "Google Calendar integration",
      "command": "npx",
      "args_template": ["-y", "@cocal/google-calendar-mcp"],
      "enabled": "${GOOGLE_CALENDAR_ENABLED}",
      "env": {
        "GOOGLE_OAUTH_CREDENTIALS": "${GOOGLE_OAUTH_CREDENTIALS}"
      }
    }
  }
}
